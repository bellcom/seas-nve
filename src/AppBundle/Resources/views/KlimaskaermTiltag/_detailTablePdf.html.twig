<table class="table table-condensed">
  <thead>
  <tr>
    {{ helper.header_span('common.generelt', 5) }}
    {{ helper.header_span('klimaskaerm.labels.singular', 4) }}
    {{ helper.header_span('common.oekonomi', 7) }}
  </tr>
  <tr>
    {{ helper.header('appbundle.tiltagdetail.tilvalgt') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.type') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.placering') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.noteGenerelt') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.klimaskaerm') }}

    {{ helper.header('appbundle.klimaskaermtiltagdetail.antalStk') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.arealM2') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.uEksWM2K') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.uNyWM2K') }}

    {{ helper.header('appbundle.klimaskaermtiltagdetail.noterTilPrisfaktorValgteLoesningTiltagSpecielleForholdPaaStedet') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.delprisKrM2') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.samletInvesteringKr') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.levetidAar') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.simpelTilbagebetalingstidAar') }}
    {{ helper.header('appbundle.klimaskaermtiltagdetail.nutidsvaerdiSetOver15AarKr') }}

    {{ helper.header() }}
  </tr>
  <tr>
    {{ helper.header_unit('appbundle.tiltagdetail.tilvalgt') }}
    {{ helper.header_unit('appbundle.tiltagdetail.type') }}
    {{ helper.header_unit('appbundle.tiltagdetail.placering') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.noteGenerelt') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.klimaskaerm', true) }}

    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.antalStk') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.arealM2') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.uEksWM2K') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.uNyWM2K', true) }}

    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.noterTilPrisfaktorValgteLoesningTiltagSpecielleForholdPaaStedet') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.delprisKrM2') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.samletInvesteringKr') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.levetidAar') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.simpelTilbagebetalingstidAar') }}
    {{ helper.header_unit('appbundle.klimaskaermtiltagdetail.nutidsvaerdiSetOver15AarKr') }}

    {{ helper.header_unit() }}
  </tr>
  </thead>
  <tbody>
  {% for detail in entity.details %}
    <tr>
      <td class="{{ detail.tilvalgt | b2class }}">{{ detail.tilvalgt | b2icon }}</td>
      <td>{{ detail.type | trans }}</td>
      <td>{{ detail.placering }}</td>
      <td>{{ detail.noteGenerelt }}</td>
      <td class="border-right">{{ detail.klimaskaerm }}</td>

      <td class="number">{{ detail.antalStk | format_integer }}</td>
      <td class="number">{{ detail.arealM2 | format_decimal }}</td>
      <td class="number">{{ detail.uEksWM2K | format_decimal }}</td>
      <td class="number border-right">{{ detail.uNyWM2K | format_decimal }}</td>

      <td>{{ detail.noterTilPrisfaktorValgteLoesningTiltagSpecielleForholdPaaStedet }}</td>
      <td class="number">{{ detail.klimaskaerm.enhedsprisEksklMoms | format_zeros }}</td>
      <td class="number">{{ detail.samletInvesteringKr | format_amount }}</td>
      <td class="number">{{ detail.levetidAar | format_zeros }}</td>
      <td class="number">{{ detail.samletInvesteringKr | format_amount }}</td>
      <td class="number">{{ detail.nutidsvaerdiSetOver15AarKr | format_amount }}</td>

      <td class="last"></td>
    </tr>
  {% endfor %}
  </tbody>
</table>
