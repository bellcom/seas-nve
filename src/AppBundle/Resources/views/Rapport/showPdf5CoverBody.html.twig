    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>

    {% if rapport.bygning.virksomhed.name is empty %}
      <br><br><br><br><br>
    {% endif %}

    <div class="text-right">
      <img src="{{ absolute_url(asset('img/seas-nve-logo-pdf.png')) }}" height="60">
    </div>
  </div>
</div>
