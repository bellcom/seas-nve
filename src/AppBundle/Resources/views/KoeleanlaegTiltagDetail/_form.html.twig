<div class="row">
  <div class="col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">{{ 'koeleanlaegtiltagdetail.groups.tilstandfoer' | trans }}</h3>
      </div>
      <div class="panel-body">
        {% for key, row in edit_form.tilstandDataFoer %}
          {{ form_row(row) }}
        {% endfor %}
      </div>
    </div>
  </div>  <div class="col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">{{ 'koeleanlaegtiltagdetail.groups.tilstandefter' | trans }}</h3>
      </div>
      <div class="panel-body">
        {{ form_row(edit_form.tilstandDataEfter.to) }}
        {{ form_row(edit_form.tilstandDataEfter.tc) }}
        {{ form_row(edit_form.tilstandDataEfter.etaC) }}
        {{ form_row(edit_form.tilstandDataEfter.etaKapa) }}
        <div class="form-group override">
          <label class="control-label">Tilsedesæt beregnet {{ 'appbundle.koeleanlaegtiltagdetail.tilstandDataEfter.ek' | trans }} værdi: {{ entity.calculateEkEfter(1) | number_format }} {{ 'appbundle.koeleanlaegtiltagdetail.tilstandDataEfter.ek.unit' | trans }} </label>
          <span class="explanation-toggle" data-container="body" data-toggle="popover" data-placement="top" data-content="{{ 'appbundle.koeleanlaegtiltagdetail.tilstandDataEfter.ek.explanation' | trans }}" data-original-title="" title=""><span class="fa fa-question-circle"></span></span>
          <div class="wrapper" style="margin-left: 20px;">
            <div class="form-group">
              <div class="checkbox">
                {{ form_widget(edit_form.tilstandDataEfter.ekOverwriteFlag) }}
              </div>
            </div>
            <div class="form-group">
              <div class="input-group">
                {{ form_widget(edit_form.tilstandDataEfter.ekOverwrite) }}
                <span class="input-group-addon">{{ 'appbundle.koeleanlaegtiltagdetail.tilstandDataEfter.ek.unit' | trans }}</span>
              </div>
            </div>
          </div>
        </div>
        {{ form_row(edit_form.tilstandDataEfter.ehjK) }}
        {{ form_row(edit_form.tilstandDataEfter.ehjV) }}
      </div>
    </div>
  </div>
</div>
