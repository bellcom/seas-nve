        <tr>
          <th class="indent"></th>
          <td class="text-right"></td>
          <td></td>
          <th>{{ 'appbundle.virksomhed_rapport.internRenteInklFaellesomkostninger' | trans }}<div class="calculation-expression hidden">{{ entity.expr('internRenteInklFaellesomkostninger') }}</div></th>
