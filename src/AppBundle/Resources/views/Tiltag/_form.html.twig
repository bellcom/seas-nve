<div class="panel panel-default">
  <div class="panel-body">
    <div class="col-xs-12 col-md-6 ">
      {% if edit_form.tilvalgtAfMagistrat is defined %}
        {{ form_row(edit_form.tilvalgtAfMagistrat, { 'label': 'appbundle.tiltagtilvalgt.tilvalgt' | trans }) }}
      {% endif %}
      <div class="hidden">
        {% if edit_form.tilvalgtAfRaadgiver is defined %}
          {{ form_row(edit_form.tilvalgtAfRaadgiver) }}
        {% endif %}

        {% if edit_form.tilvalgtAfAaPlus is defined %}
          {{ form_row(edit_form.tilvalgtAfAaPlus) }}
        {% endif %}
        {% if edit_form.tilvalgtbegrundelse is defined %}
          {{ form_row(edit_form.tilvalgtbegrundelse) }}
        {% endif %}

        {% if edit_form.tilvalgtbegrundelse is defined %}
          {{ form_row(edit_form.tilvalgtBegrundelseMagistrat) }}
        {% endif %}
      </div>
    </div>
  </div>
</div>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">{{ 'common.generelt' | trans }}</h3>
  </div>
  <div class="panel-body">
    <div class="row" style="margin:0">
      <div class="col-xs-12 col-md-6">
        {{ form_row(edit_form.title) }}
        {{ form_row(edit_form.placering) }}

        {% if edit_form.datoForDrift is defined %}
          {{ form_row(edit_form.datoForDrift) }}
        {% endif %}

        {% if edit_form.slutanvendelse is defined %}
          {{ form_row(edit_form.slutanvendelse) }}
        {% endif %}
      </div>
      <div class="col-xs-12 col-md-6">
        {% if edit_form.forbrugFoerVarme is defined %}
          {{ form_row(edit_form.forbrugFoerVarme) }}
        {% endif %}
        {% if edit_form.forbrugFoerEl is defined %}
          {{ form_row(edit_form.forbrugFoerEl) }}
        {% endif %}
        {% if edit_form.forbrugFoerBraendstof is defined %}
          {{ form_row(edit_form.forbrugFoerBraendstof) }}
        {% endif %}
        {% if edit_form.levetid is defined %}
          {{ form_row(edit_form.levetid) }}
        {% endif %}
      </div>
    </div>
    <div class="row" style="margin:0">
      <div class="col-xs-12 col-md-6">
        {{ form_row(edit_form.forsyningVarme) }}
        {% if edit_form.priserOverride.varme is defined %}
          <div class="form-group override">
            <label class="control-label">{{ 'appbundle.tiltag.priserOverride.varme' | trans }}: {{ entity.getRapportVarmeKrKWh | number_format }} {{ 'appbundle.forsyningsvaerk.pris.unit' | trans }} </label>
            {{ form_widget(edit_form.priserOverride.varme) }}
          </div>
        {% endif %}
        {% if edit_form.co2Override.varme is defined %}
          <div class="form-group override">
            <label class="control-label">{{ 'appbundle.tiltag.co2Override.varme' | trans }}: {{ entity.getRapportVarmeKgCo2MWh | number_format }} {{ 'appbundle.forsyningsvaerk.co2.unit' | trans }} </label>
            {{ form_widget(edit_form.co2Override.varme) }}
          </div>
        {% endif %}
      </div>
      <div class="col-xs-12 col-md-6">
        {{ form_row(edit_form.forsyningEl) }}
        {% if edit_form.priserOverride.el is defined %}
          <div class="form-group override">
            <label class="control-label">{{ 'appbundle.tiltag.priserOverride.el' | trans }}: {{ entity.getRapportElKrKWh | number_format }} {{ 'appbundle.forsyningsvaerk.pris.unit' | trans }} </label>
            {{ form_widget(edit_form.priserOverride.el) }}
          </div>
        {% endif %}
        {% if edit_form.co2Override.el is defined %}
          <div class="form-group override">
            <label class="control-label">{{ 'appbundle.tiltag.co2Override.el' | trans }}: {{ entity.getRapportElKgCo2MWh | number_format }} {{ 'appbundle.forsyningsvaerk.co2.unit' | trans }} </label>
            {{ form_widget(edit_form.co2Override.el) }}
          </div>
        {% endif %}
      </div>
    </div>
  </div>
</div>

