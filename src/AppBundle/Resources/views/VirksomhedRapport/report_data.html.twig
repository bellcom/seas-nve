      <div class="panel-heading"><h3 class="panel-title">{{ icon('line-chart') }} {{ 'appbundle.virksomhed_rapport.akkumuleretNutidsvaerdiSet' | trans }}</h3></div>

      <div class="panel-body">
        {%  set linje_graf_label = (('appbundle.virksomhed_rapport.akkumuleretNutidsvaerdiSet' | trans) ~ ', ' ~ ('appbundle.virksomhed_rapport.akkumuleretNutidsvaerdiSet.unit' | trans)) %}
        {% include 'AppBundle::_includes/linje_graf_years.html.twig' with { 'linje_graf_data' : entity.akkumuleretNutidsvaerdiSet } %}
      </div>
    </div>

    <div class="panel panel-default">
      <div class="panel-heading"><h3 class="panel-title">{{ icon('dollar') }} {{ 'common.finans' | trans }}</h3></div>

      <table class="table table-condensed">
        <tbody>
        <tr>
          <th>{{ 'appbundle.virksomhed_rapport.faellesomkostninger' | trans }}:</th>
          <td></td>
          <td></td>
          <th>{{ 'appbundle.virksomhed_rapport.totalEntreprisesum' | trans }}<div class="calculation-expression hidden">{{ entity.expr('anlaegsinvestering') }}</div></th>
          <td class="text-right">{{ entity.anlaegsinvestering | format_amount }}</td>
          <td>{{ 'appbundle.virksomhed_rapport.totalEntreprisesum.unit' | trans }}</td>
        </tr>
        <tr>
