{% block virksomhed_embed -%}
  <strong>{{ entity }}</strong><br/>
  {% if entity.adress %}
    {{ entity.adress }}<br/>
  {% endif %}
  {% if entity.postnummer or entity.byNavn %}
    {{ entity.postnummer }}{{ entity.byNavn }}<br/>
  {% endif %}
  {% if entity.kommune %}
    {{ entity.kommune }}<br/>
  {% endif %}
  {% if entity.region %}
    {{ entity.region }}<br/>
  {% endif %}
  {% if entity.cvrNumber %}
    CVR nr.: {{ entity.cvrNumber }}<br/><br/>
  {% endif %}
  {% if entity.contactPersons[0].name %}
    Kontaktperson: {{ entity.contactPersons[0].name }}<br/>
  {% endif %}
  {% if entity.contactPersons[0].phoneNumber %}
    Telefon: {{ entity.contactPersons[0].phoneNumber }}<br/>
  {% endif %}
  {% if entity.contactPersons[0].mail %}
    Email: <a href="mailto:{{ entity.contactPersons[0].mail }}">{{ entity.contactPersons[0].mail }}</a>
  {% endif %}
{% endblock %}
