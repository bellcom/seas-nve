{% block virksomhed_embed_overview -%}
  <div>
    <strong>{{ entity }}</strong>
  </div>
  {% for bygning in entity.getBygninger() %}
    {{ bygning }}, {{ bygning.adresse }}, {{ bygning.postnummer }} {{ bygning.postBy }}
    {% if bygning.rapport.getTilvalgteTiltag() is not empty %}
      <ol>
        {% for tiltag in bygning.rapport.getTilvalgteTiltag() %}
          <li>{{ tiltag }}</li>
        {% endfor %}
      </ol>
    {% else %}
      <p>Ingen tiltag i {{ entity.name }} virksomhed rapport</p>
    {% endif %}
  {% endfor %}
{% endblock %}
